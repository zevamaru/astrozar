{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/server.js" },
    { "source": "/cosmos/(.*)", "destination": "/server.js" },
    { "source": "/:asset*", "destination": "/public/:asset*" }
  ]
}
